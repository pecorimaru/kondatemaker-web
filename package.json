{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://apiapi.nagoya",
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "humps": "^2.0.1",
    "kuroshiro": "^1.2.0",
    "kuroshiro-analyzer-kuromoji": "^1.1.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.26.1",
    "swr": "^2.2.5",
    "vanilla-autokana": "^1.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:vite\"",
    "dev:css": "tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "dev:vite": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "start": "vite"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^4.9.5",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
